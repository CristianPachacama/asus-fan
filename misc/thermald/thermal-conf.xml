                                                                       <CoolingDevice>
                                                                                       <index>1</index>
                                                                                       <type>CPUFan</type>
                                                                                       <influence>100</influence>
                                                                                       <SamplingPeriod>1</SamplingPeriod>
                                                                       </CoolingDevice>
                                                                       <CoolingDevice>
                                                                                       <index>2</index>
                                                                                       <type>GFXFan</type>
                                                                                       <influence>100</influence>
                                                                                       <SamplingPeriod>1</SamplingPeriod>
                                                                       </CoolingDevice>
                                                       </TripPoint>
                                       </TripPoints>
                       </ThermalZone><ThermalZone>
                                       <Type>tdp_zone</Type>
                                       <TripPoints>
                                                       <TripPoint>
                                                                       <SensorType>die_sens</SensorType>
                                                                       <Temperature>87000</Temperature>
                                                                       <type>passive</type>
                                                                       <ControlType>SEQUENTIAL</ControlType>
                                                                       <CoolingDevice>
